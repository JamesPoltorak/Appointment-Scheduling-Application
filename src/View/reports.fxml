<AnchorPane prefHeight="656.0" prefWidth="909.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Reports">
   <children>
      <Label layoutX="172.0" layoutY="14.0" text="Reports">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <GridPane alignment="CENTER" layoutX="34.0" layoutY="112.0" prefHeight="402.0" prefWidth="351.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="customer_type" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#customer_type_click" prefHeight="54.0" prefWidth="146.0" text="Customer Appointments by Type" textAlignment="CENTER" wrapText="true" />
            <Button fx:id="contact_schedule" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#contact_schedule_click" prefHeight="35.0" prefWidth="184.0" text="Contact Schedule" GridPane.columnSpan="2" GridPane.rowIndex="1" />
            <Button fx:id="appointments_location" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#appointments_location_click" prefHeight="35.0" prefWidth="184.0" text="Appointments By Location" GridPane.columnSpan="2" GridPane.rowIndex="2" />
            <Button fx:id="customer_month" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="50.0" mnemonicParsing="false" onAction="#customer_month_click" prefHeight="54.0" prefWidth="146.0" text="Customer Appointments by Month" textAlignment="CENTER" wrapText="true" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="back" layoutX="181.0" layoutY="575.0" mnemonicParsing="false" onAction="#back_click" prefHeight="35.0" prefWidth="57.0" text="Back" />
      <ScrollPane layoutX="436.0" layoutY="147.0" prefHeight="473.0" prefWidth="422.0">
         <content>
            <Label fx:id="report_field" />
         </content>
      </ScrollPane>
   </children>
</AnchorPane>
